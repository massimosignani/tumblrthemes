<script type="text/javascript">
    function recent_tweets(data) {
        for (i=0; i<1; i++) {
            var date = moment(data[i].created_at);
            var timeago = moment(date).fromNow();
            document.getElementById("tweets").innerHTML =
                document.getElementById("tweets").innerHTML +
                '<li><a class="tweet" target="_blank" href="http://twitter.com/'+twitterUsername+'/status/' +
                (data[i].id_str ? data[i].id_str : data[i].id) +
                '">' + data[i].text +
                '</a><a class="time-ago" target="_blank" href="http://twitter.com/'+twitterUsername+'/status/' +
                (data[i].id_str ? data[i].id_str : data[i].id) +
                '">&mdash; ' + timeago + '</a></li>';
        }
        document.getElementById("twitter").style.display = 'block';
    }
</script>
<script type="text/javascript" src="/tweets.js"></script>
<script type="text/javascript">
    $("#instafeed").text("loading");
        $.ajax({
        url: "https://api.instagram.com/v1/users/self/media/recent",
        data: { access_token: instagramAccessToken },
        dataType: "jsonp",
        error: function(jqXHR, textStatus, errorThrown) {
        $("#instafeed").text(textStatus);
        },
        success: function(data, textStatus, jqXHR) {
        $("#instafeed").text("");
        for (var i = 0, length = 6; i < length; i++) {
        	var d = data.data[i];
            $("#instafeed").append("<li><a href='"+d.link+"' target='_blank' style='background-image: url("+d.images.standard_resolution.url+");'></a></li>");}
        }
    });
</script>
<script type="text/javascript">
$.jribbble.setToken(dribbbleAccessToken);

$.jribbble.users(dribbbleUsername).shots({per_page: 6}).then(function(shots) {
  var html = [];
  
  shots.forEach(function(shot) {
    html.push('<li class="shot feed-item">');
    html.push('<a href="' + shot.html_url + '" target="_blank" style="background-image: url(' + shot.images.normal + ');">');
    html.push('</a></li>');
  });
  
  $('#dribbble-shots').html(html.join(''));
});
</script>
