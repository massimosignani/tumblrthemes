<!---------LIGHTBOX------>
<script>

        function divFunction(){
            jQuery(".photo_cap_inner").fadeIn();
        }
        function closeFunction(){
            jQuery(".photo_cap_inner").fadeOut();
        }
        
    jQuery(document).ready(function() {
        //FancyBox Start
        jQuery(".fancybox").fancybox({
    	openEffect	: 'elastic',
    	closeEffect	: 'elastic',
    	 afterLoad: function(){
           this.title = jQuery(this.element).next('.photo_cap').html();
          },
          afterClose:function(){
              jQuery(".photo_cap_inner").css({"display":"none"});
          },
          helpers: {
                title : {
                type : 'inside'
                }
           }
          });
        //FancyBox END  

          
        //jQuery(".single_page:contains(route=custom_page)").addClass('page_body');
        jQuery(".single_page .photo .caption").has('.tumblr_blog').prepend('<div class="post_icon"><i class="fa fa-retweet"></i></div>');
        
        jQuery("#single_notes .reblog .action").after('<i class="fa fa-retweet"></i>');
        jQuery("#single_notes .like .action").after('<i class="fa fa-heart"></i>');
        
        //THE POPUP BOX
        jQuery(".about_buttn").click(function(){
			jQuery('.popup_text, .blank_canvas').fadeIn();	
		});
		jQuery(".popup_text .fa-times-circle-o, .blank_canvas").click(function(){
			jQuery('.popup_text, .blank_canvas').fadeOut();	
		});
		
		//MOBILE MENU

			jQuery('.mobile_menu').toggle(
			  function(){
				 jQuery("#nav").slideDown(100);
			  },
			  function(){
				 jQuery("#nav").slideUp(100);
			  });
			  

    });
    
    // TO_TOP
    jQuery(window).bind("scroll", function() {
        if (jQuery(this).scrollTop() > 700) {
            jQuery(".to_top").fadeIn('slow');
        } else {
            jQuery(".to_top").fadeOut('fast');
        }
    });
    jQuery(".to_top").click(function() {
      jQuery("html, body").animate({ scrollTop: 0 }, "slow");
      return false;
    });

</script>


<script>
    (function () {
    var $tumblelog = $('.home_page section.content');
    $tumblelog.infinitescroll({
        navSelector: ".home_page .pagination",
        nextSelector: ".home_page .pagination a:last-child",
        itemSelector: ".athnrl_blck",
         loading: {
            img  : "http://static.tumblr.com/aug1ktz/7GCn2a9lk/377.gif"}
    },

    function (newElements) {
        var $newElems = $(newElements).css({
            opacity: 0
        });
        $newElems.imagesLoaded(function () {
            $newElems.animate({
                opacity: 1
            });
            $tumblelog.masonry('appended', $newElems);
        });
    });
    $tumblelog.imagesLoaded(function () {
        $tumblelog.masonry({
            columnWidth: function (containerWidth) {
                return containerWidth / 100;
            }
        });
    });
})();
    
</script>

