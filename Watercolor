<script type="text/javascript">
     $(document).ready(function(){
        var textLength = $("h1 a").text().length;
        if ((textLength > 17) && (textLength < 22)) {
           $("h1").css('fontSize', 33);
        }
        else if ((textLength >= 22) && (textLength < 25)) {
           $("h1").css('fontSize', 29);
        }
        else if (textLength >= 25) {
           $("h1").css('fontSize', 25);
        }
         
 });
</script>
