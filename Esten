            <div id="ft-con" class="ft-con 
                {block:IfShowFooterTwitterWidget} widget{/block:IfShowFooterTwitterWidget}
                {block:IfShowFooterTextWidget1} widget{/block:IfShowFooterTextWidget1}
                {block:IfShowFooterTextWidget2} widget{/block:IfShowFooterTextWidget2}
