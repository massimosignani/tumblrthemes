        <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
        <script src="https://static.tumblr.com/8wlin78/fA9n7ywrj/masonry.pkgd.min.js"></script>
        <script src="https://static.tumblr.com/8wlin78/Dlmoaejg1/moment.min.js"></script>      
        <script src="https://static.tumblr.com/8wlin78/Q0rncaorg/livestamp.min.js"></script>
        <script src="https://static.tumblr.com/8wlin78/zpjncaoom/jquery.photoset-grid.js"></script>
        <script src="https://static.tumblr.com/8wlin78/O1Inimeia/socialstream.jquery.js"></script>
        <script src="https://static.tumblr.com/8wlin78/C5Wno0utj/imagesloaded.pkgd.min.js"></script>
        <script src="https://static.tumblr.com/8wlin78/Ll4ncaot0/nivo-lightbox.min.js"></script>
        <script src="https://static.tumblr.com/8wlin78/Xn7oacp7q/estentheme.min.js"></script>      
        
        <script>
            $(document).ready(function(){
                /*Theme script init*/
                $(document).estentheme({
                   disqusID: '{text:Disqus Shortname}',
                   sliderDelay: '{text:Header image rotation delay}'
                });
                /*Masonry init */
                var container = document.querySelector('#posts');
                msnry = new Masonry( container, {
                    transitionDuration: 0,
                    itemSelector: '.element',
                    columnWidth: 324,
                });
                var ftcontainer = document.querySelector('#ft-con');
                ftmsnry = new Masonry( ftcontainer, {
                    transitionDuration: 0,
                    itemSelector: '.ft-section',
                    columnWidth: container.querySelector('.ft-section'),
                });
                /*Aspect ratio resize*/
                setAspect = function() {
                    var iframes = $('.typography iframe, .video-wrapper iframe, .video-wrapper video, .audio-wrapper iframe');
                    for (var i = 0; i < iframes.length; i++) {
                        var ci = $(iframes[i]),
                            ciW = ci.attr('width'),
                            ciH = ci.attr('height'),
                            aspect = ciH / ciW,
                            parentW = ci.parent().width();
                        ci.css({"width": parentW, "height": parentW * aspect});
                    }   
                }
                setAspect();
                $(window).resize(function() {
                    setAspect();
                    msnry.layout();
                    ftmsnry.layout();
                });
                
                // Masonry layout
                setInterval(function() {
                    msnry.layout();
                }, 2000);

                
                var footerFeeds = $('.social-feed');
                if (footerFeeds.length) {
                    setInterval(function() {
                        ftmsnry.layout();
                    }, 5000);
                }
                
                scLoad = function() {
                    var images = $('figure img');
                    for (var j = 0; j < images.length; j++) {
                        var i = $(images[j]),
                            iSrc = i.attr('src');
                        if (!i.is('.wraped')) {
                            i.wrap('<a href="' + iSrc + '" class="nivo-lb" />');
                            i.addClass('wraped');
                       }
                    }
                    $('a.nivo-lb').nivoLightbox({
                        effect: 'fadeScale'
                    });
                    var photosets = $('.photoset-grid');
                    imagesLoaded(photosets, function() {
                        photosets.photosetGrid({
                            highresLinks: true,
                            gutter: '3px',
                            
                            onComplete: function(){
                                var photosets = $('.photoset-grid');
                                for (var i = 0; i < photosets.length; i++) {
                                    var curSet = $(photosets[i]),
                                        pictures = curSet.find('a'),
                                        photosetID = curSet.attr('data-id');
                                    pictures.attr('data-lightbox-gallery', photosetID)
                                    .nivoLightbox({
                                        effect: 'fadeScale'
                                    });
                                }
                            }
                        });
                        msnry.layout();
                    });
                    var images = $('img');
                    imagesLoaded(images, function() {
                        msnry.layout();
                    });
                    setAspect();
                }    
                scLoad();

                /*Instagram Feed*/
                {block:IfInstagramAccessToken}
                var InstaToken = '{text:Instagram access token}',     
                    InstaImageLimit = '{text:instagram image limit}',
                    InstaFeed = $('.instagram-feed'),
                    InstaFeedUl = $('<ul/>').addClass('instagram-list').appendTo(InstaFeed);
                $.ajax({
                    url: 'https://api.instagram.com/v1/users/self/media/recent/?access_token=' + InstaToken,
                    dataType: 'jsonp'
                })
                    .done(function (r) {
                        $.each(r.data, function (i, item) {
                            if (i < InstaImageLimit) {
                                var imgCap = item.caption,
                                    imgAlt = '';
                                if (imgCap) {
                                    imgAlt = item.caption.text;
                                }
                                $('<img/>').attr({
                                    alt: imgAlt,
                                    src: item.images.thumbnail.url
                                })
                                    .appendTo(InstaFeedUl).wrap('<li><a href="' + item.link + '" title="' + imgAlt + '"></a></li>');
                            }
                        })
                    })
                {/block:IfInstagramAccessToken}
                /*Flickr Feed*/
                {block:IfFlickrUsername}
                $('.flickr-feed').socialstream({
                    socialnetwork: 'flickr',
                    limit: {text:Flickr image limit},
                    username: '{text:Flickr username}'
                });
                {/block:IfFlickrUsername}
                
            });
        </script>
        
