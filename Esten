                <div class="ft-section twitter-feed" id="twitter-feed">
                    <div class="ft-label">
                        <span>{text:Twitter feed label}</span>
                    </div>
                    <div id="tweets" class="tweets"></div>
                </div>    
                <script type="text/javascript">
                    function recent_tweets(data) {
                        String.prototype.parseURL = function() {
                            return this.replace(/[A-Za-z]+:\/\/[A-Za-z0-9-_]+\.[A-Za-z0-9-_:%&\?\/.=]+/g, function(url) {
                                return url.link(url);
                            });
                        };
                        String.prototype.parseUsername = function() {
                            return this.replace(/[@]+[A-Za-z0-9-_]+/g, function(u) {
                            var username = u.replace("@","")
                                return u.link("http://twitter.com/" + username);
                            });
                        };
                        String.prototype.parseHashtag = function() {
                            return this.replace(/[#]+[A-Za-z0-9-_]+/g, function(t) {
                            var tag = t.replace("#","%23")
                                return t.link("https://twitter.com/hashtag/" + tag + "?src=hash");
                            });
                        }; 
                        for (i=0; i<{text:Twitter feed limit}; i++) {
                            document.getElementById("tweets").innerHTML =
                                document.getElementById("tweets").innerHTML +
                                '<div class="tweet"><i class="icon-twitter"></i><div class="tweet-date" data-livestamp="' + data[i].created_at + '"></div><div class="tweet-text">' + data[i].text.parseURL().parseUsername().parseHashtag() + '</div></div>';
                        }
                    }
                </script>
                {/block:Twitter}{block:IfShowFooterTwitterWidget}
