    <script src="//static.tumblr.com/zb6te2b/vE0ofv8nf/salvattore.min.js"></script>
    <script src="//static.tumblr.com/zb6te2b/WbOnkv3x8/jquery.infinitescroll.min.js"></script>
    <script>
        (function () {
            var acme = document.getElementById("acme"),
                pages = document.getElementById("acme-pages"),
                posts = document.getElementById("acme-posts"),
                previous;

            window.addEventListener("mousemove", function (event) {
                var target = event.target;

                if (event.clientX <= document.body.clientWidth) {
                    do {
                        if (target.classList.contains("acme-post")) {
                            if (previous !== true) {
                                posts.classList.remove("-active");
                                pages.classList.remove("-active");
                            }
                            previous = true;
                            return true;
                        } else if (target.classList.contains("acme-header-pages-wrap-activate")) {
                            posts.classList.add("-active");
                            pages.classList.add("-active");
                            return true;
                        } else {
                            target = target.parentElement;
                        }
                    } while (target.parentElement);
    
                    if (previous !== false) {
                        posts.classList.add("-active");
                        pages.classList.remove("-active");
                    }
                    previous = false;
                    return false;
                }
            });

            function toggle(element, classname) {
                if (element.classList.contains(classname)) {
                    element.classList.remove(classname);
                } else {
                    element.classList.add(classname);
                }
            }

            function lightbox(event) {
                var target = event.target;

                do {
                    if (target.classList.contains("acme-lightbox-activate")) {
                        toggle(acme, "-lightbox-active");
                        return true;
                    } else {
                        target = target.parentElement;
                    }
                } while (target.parentElement);

                if (acme.classList.contains("-lightbox-active")) {
                    acme.classList.remove("-lightbox-active");
                }
                return false;
            }

            window.addEventListener("click", function (event) {
                lightbox(event);
            });

            /*window.addEventListener("touchstart", function (event) {
                lightbox(event);
            });*/
        }());

        $(document).ready(function () {
            /*
            $(".acme-lightbox-activate").click(function (event) {
                event.preventDefault();
                $(".acme-lightbox").load($(this).attr("href") + " #" + $(this).data("permalink-id"));
            });
            */
            window.slider = $('.acme-post-media-photoset').unslider({
                animateHeight: true
            });
        });
        
        /*
        $(document).infinitescroll({
          navSelector    : ".acme-pagination",
          nextSelector   : ".acme-pagination-next",
          contentSelector: ".acme-posts",
          itemSelector   : ".acme-post",
          loadingText    : "Loading new posts...",
          donetext       : "No more posts"
        }, function(arrayOfNewElems) {
            salvattore.appendElements(document.getElementById("acme-posts"), arrayOfNewElems);
        });
        */
        
        $.extend($.infinitescroll.prototype, {
            _setup_click: function infscr_setup_click() {
                var instance = this,
                    opts = this.options;
                this.options.loading.start = function(opts) {
                    $(opts.nextSelector).on("click", function(e) {
                        e.preventDefault();
                        $(opts.textSelector).fadeOut(250);
                        opts.loading.msg.appendTo(opts.loading.selector).fadeIn(250);
                        window.setTimeout(function() {
                            instance.beginAjax(opts);
                        }, 2500);
                    });
                };
                this.options.loading.finished = function() {
                    opts.loading.msg.fadeOut(500);
                    $(opts.textSelector).delay(1000).fadeIn("slow");
                };
                return false;
            },
            _showdonemsg: function infscr_showdonemsg() {
                var opts = this.options;
                if (!!opts.behavior && this["_showdonemsg_" + opts.behavior] !== undefined) {
                    this["_showdonemsg_" + opts.behavior].call(this);
                    return;
                }
                opts.loading.msg.find("img").hide().parent().find("div").html(opts.loading.finishedMsg).animate({
                    opacity: 1
                }, 2000, function() {
                    $(opts.navSelector).fadeOut("fast");
                });
                opts.errorCallback.call($(opts.contentSelector)[0], "done");
            }
        });
        $(".acme-body").infinitescroll({
            loading: {
                selector: ".acme-pagination",
                finishedMsg: "{lang:No more posts}.",
                msgText: "{lang:Loading}",
                img: "data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7"
            },
            navSelector    : ".acme-pagination",
            nextSelector   : ".acme-pagination-next",
            contentSelector: ".acme-posts",
            itemSelector   : ".acme-post",
            appendCallback: false
        }, function(items) {
            salvattore.appendElements(document.getElementById("acme-posts"), items);
            window.slider = $('.acme-post-media-photoset').unslider({
                animateHeight: true
            });
            var ids = $(items).map(function() {
                return this.id;
            }).get();
            Tumblr.LikeButton.get_status_by_post_ids(ids);
        });
        $(".acme-body").infinitescroll("retrieve");
    </script>
    {/block:IndexPage}
    <script>
        $(document).imagesLoaded().always(function() {
            var iframes = document.querySelectorAll('.acme-post-media > iframe'),
                i;
                
            for (i = 0; i < iframes.length; i++) {
        
                var iframe = iframes[i],
                    videoRatio = (iframe.height / iframe.width) * 100;
        
                    /*
                     * Replace the iframe's dimensions and position
                     * the iframe absolute, this is the trick to emulate
                     * the video ratio
                     */
                    iframe.style.position = 'absolute';
                    iframe.style.top = '0';
                    iframe.style.left = '0';
                    iframe.width = '100%';
                    iframe.height = '100%';
        
                    /*
                     * Wrap the iframe in a new <div> which uses a
                     * dynamically fetched padding-top property based
                     * on the video's w/h dimensions
                     */
                    var wrap = document.createElement('div');
                    wrap.className = 'fluid-vids';
                    wrap.style.width = '100%';
                    wrap.style.position = 'relative';
                    wrap.style.paddingTop = videoRatio + '%';
        
                    /*
                     * Add the iframe inside our newly created <div>
                     */
                    var iframeParent = iframe.parentNode;
                    iframeParent.insertBefore(wrap, iframe);
                    wrap.appendChild(iframe);
            }
            $(".acme").addClass("-loaded");
        });
    </script>
</body>

