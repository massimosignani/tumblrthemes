        $("#toTop").text("Back to Top");
      }else if ($(window).width() <= 641){
         // $("#toTop").text("^")
          $('a:contains("Back to Top")').html('<img src="http://static.tumblr.com/4w8bepz/6cInho36d/backtotop.png"/>')
      } 
    };
    
    $(window).resize(resizeButton);
    resizeButton();
    
</script>


</body>
</html>
