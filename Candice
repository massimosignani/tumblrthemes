        <!-- start modals-->                                  
        <div id="searchModal" class="reveal full" data-reveal>
            <div class="search-container">
                <div class="search-wrapper">
                    <h1>{lang:Search}:</h1>
                    <form action="/search" method="get" class="nav-search row small-12 medium-8">
                        <input class="columns small-10 medium-11" type="text" name="q" value="{SearchQuery}" id="search-input" placeholder="{lang:Search keywords}"/>
                        <button class="columns small-2 medium-1" type="submit"><i class="fa fa-search"></i></button>
                    </form>
                </div>
            </div>
            <a class="close-modal-button" data-close><span class="cross-ico"></span></a>
        </div>
        
        {block:IfPinterestBoardURL}
        <script type="text/javascript" async src="https://assets.pinterest.com/js/pinit.js"></script>
        {/block:IfPinterestBoardURL}
        <script src="https://static.tumblr.com/cmghtlm/iwbnprdu8/jquery.js"></script>
        <script src="https://static.tumblr.com/honophf/ptep2xtsc/candice_plugins.js"></script>
        <script src="https://static.tumblr.com/vppzax2/zsgo35ewv/tweenmax.min.js"></script> 
        <script src="https://static.tumblr.com/vppzax2/N8wo35et5/scrolltoplugin.min.js"></script>
        <script>
            
            $(document).foundation();
                        
            enquire.register("screen and (min-width: 64.063em)", {
                match : function() { 
                    {block:IfNotDisableSmoothScroll}                   
                    //smooth scroll
                    var $window = $(window);
                    var scrollTime = 0.7;
                    var scrollDistance = 300;
    
                    $window.on("mousewheel DOMMouseScroll", function(event){
    
                        event.preventDefault(); 
    
                        var delta = event.originalEvent.wheelDelta/120 || -event.originalEvent.detail/3;
                        var scrollTop = $window.scrollTop();
                        var finalScroll = scrollTop - parseInt(delta*scrollDistance);
    
                        TweenMax.to($window, scrollTime, {
                            scrollTo : { y: finalScroll, autoKill:true },
                                ease: Power1.easeOut,
                                overwrite: 5                            
                            });
                    });
                    {/block:IfNotDisableSmoothScroll}
                }
            });
                
            //scroll reveal
            var config = {
                viewFactor : 0.15,
                duration   : 800,
                distance   : "0px",
                scale      : 0,
                reset       : true,
                viewOffset: { top: 0 }
            };

            {block:IfNotDisableScrollReveal}
            window.sr = ScrollReveal( config );
            sr.reveal('.ani-div');
            {/block:IfNotDisableScrollReveal}
            
            //pre loader
            window.addEventListener('DOMContentLoaded', function() {
                new QueryLoader2(document.querySelector("body"), {
                    barColor: "{color:Loading Screen Bar Color}",
                    backgroundColor: "{color:Loading Screen Background Color}",
                    percentage: false,
                    barHeight: 2,
                    minimumTime: 200,
                    fadeOutTime: 1000
                });
            });          

            //instagram Feed
            {block:IfInstagramID}
            $("#instagram-feed").jqinstapics({
                "user_id": "{text:Instagram ID}",
                "access_token": "{text:Instagram Token}",
                "count": {text:Instagram Feed Count}
            });
            {/block:IfInstagramID} 
            
            //infinite scroll
            (function () {                
                var infiniteContainer = new InfiniteScroll( '.content-area', {
                    nextSelector: "#load-more a",
                    path: "#load-more a",
                    append: ".post",
                    status: '.page-load-status',
                    {block:IfNotInfiniteScroll}
                    button: '#load-more a',
                    scrollThreshold: false
                    {/block:IfNotInfiniteScroll}
                });

                infiniteContainer.on( 'append', function( response, path, items ) {
                    var $newElemsIDs = $(items).map(function () { return this.id; }).get();
                    
                    //get like status for posts
                    Tumblr.LikeButton.get_status_by_post_ids($newElemsIDs);
                        
                    //scroll reveal
                    sr.sync();
                    
                    //foundation reflow
                    $(document).foundation();
 
                    //magnific Popup Lightbox
                    $('.lightbox').each(function() {
                         $(this).magnificPopup({ 
                            delegate: 'a.photo-link',
                            type:'image', 
                            gallery: {
                                enabled: true,
                                navigateByImgClick: true,
                            }
                        });
                    });
                    
                    //responsive photosets
                    resizePhotosets();
                });

                infiniteContainer.on( 'last', function( response, path ) {
                    $('#load-more').remove();
                });
            })();

            //responsive photoset
            function resizePhotosets(){
                var parentWidth = document.body.querySelector('.html_photoset').offsetWidth; 
                var photosets = document.body.querySelectorAll('iframe.photoset');
                for(var i = 0; i < photosets.length; ++i){
                    var photoset = photosets[i];
                    photoset.width = parentWidth;
                }   
            }
            window.onload = resizePhotosets;
            window.onresize = resizePhotosets; 
            
            {block:IfDisqusShortname}{block:PermalinkPage}
            //disqus Comments
            var disqus_shortname = '{text:Disqus Shortname}'; 
            var disqus_url = '{Permalink}'; 

            (function() {
                var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                dsq.src = 'https://' + disqus_shortname + '.disqus.com/embed.js';
                (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
            })();
            {/block:PermalinkPage}{/block:IfDisqusShortname}
            
        </script>
        
    </body>
    
</html>
