<script>

            var oddFootColor = '{select:Odd Foot Color}';
            var evenFootColor = '{select:Even Foot Color}';
        
            var body = document.getElementById('body');
            var children = body.children;
        
            for(var i = children.length - 1; i >= 0; --i)
            {
                var child = children[i];
                var likes = child.getElementsByClassName('like_toggle');
                for(var j = likes.length - 1; j >= 0; --j)
                {
                    var color;
                    if(i%2 == 0)
                    {
                        color = oddFootColor;
                    }
                    else
                    {
                        color = evenFootColor;
                    }
                    
                    //reload like button iframe
                    likes[j].src = likes[j].src.replace('grey', color);
                }
            }

            var likes = document.getElementsByClassName('like_button');
            for(var i = likes.length - 1; i >= 0; --i)
            {
                
                var frame = likes[i].innerHTML;
                likes[i].innerHTML = frame;
            }

</script>
